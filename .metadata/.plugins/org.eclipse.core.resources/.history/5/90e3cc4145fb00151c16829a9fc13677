package laba2;

import java.util.ArrayList;

public class Generetic1 {

	public ArrayList<Tweet> Generetic(Tweet object,String stroka){
		
		ArrayList<Tweet> vasya= new ArrayList<Tweet>();
		
		String str = object.getText();
		
		if(str.contains(stroka)) {object.toString();vasya.add(object);};
		return vasya;
	}
}

