package laba2;

import java.awt.List;

public class States{
private String shtat;
private List shir;
private List dolg;

public String getShtat(){
	return shtat;
}

public List getShir(){
return shir;
}

public List getDolg(){
return dolg;
}

public void setShtat( String shtat ){
this.shtat=shtat;
}
public void setShir( List shir ){
this.shir=shir;
}

public void setDolg( List dolg ){
this.dolg=dolg;
}
public String toString(){
	System.out.println(shtat+", "+shir+", "+dolg);
	return null;
	}
}