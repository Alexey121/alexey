public String shtat="";
statesParser object = new statesParser();

public String getShtat2(){
	return shtat;
}
public ArrayList<Double> getShirota(){
	return coord.shirota;
}
public ArrayList<Double> getDolgota(){
	return coord.dolgota;
}


